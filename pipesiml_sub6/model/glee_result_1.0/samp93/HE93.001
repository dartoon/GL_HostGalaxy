    esource_sub_esr_psf  psf-s-93.fits
    esource_sub_esr_psf_factor     1
    esource_regtype      curv
    esource_regopt       SpecRegPrecSigFigOnce
    esource_reglam       488.281
    esource_reglamlo     1e-12
    esource_reglamhi     100000
    esource_reglampre    1
    esource_reglamnup    1000
    esource_light  3
        sersic
            3.983608  #x-coord   link:lv1p1  a:0,1,1
            3.937085  #y-coord   link:lv1p2  a:0,1,1
            0.917817  #q         flat:0.3,1  step:0.03
            2.047116  #PA        noprior:  step:0.1
