# glee -M -i HE93

chi2type           16
minimiser       siman
seed                2

siman_iter          1
siman_nT         1000
siman_dS            5
siman_Sf          1.1
siman_k             2
siman_Ti           10
siman_Tf          1.2
siman_Tmin          1

mcmc_n          15000
mcmc_dS        0.45
mcmc_dSini          0
mcmc_k              2

sampling_f     gaussian
sampling_cov   HE93.cov

lenses_vary 2

    spemd
    3.983608  #x-coord   gaussian:3.94,0.08  label:lv1p1     step:0.08
    3.937085  #y-coord   gaussian:3.94,0.08  label:lv1p2     step:0.08
