    esource_sub_esr_psf  psf-s-17.fits
    esource_sub_esr_psf_factor     1
    esource_regtype      curv
    esource_regopt       SpecRegPrecSigFigOnce
    esource_reglam       3.8147
    esource_reglamlo     1e-12
    esource_reglamhi     100000
    esource_reglampre    1
    esource_reglamnup    1000
    esource_light  3
        sersic
            3.928291  #x-coord   link:lv1p1  a:0,1,1
            3.914951  #y-coord   link:lv1p2  a:0,1,1
            0.995199  #q         flat:0.3,1  step:0.03
            1.912681  #PA        noprior:  step:0.1
            0.137056  #amp       noprior:  min:0.001  step:0.1
            2.498608  #r_eff     flat:1.8,2.5  step:0.05
            4.001594  #n_sersic  flat:4,6  step:0.1
        psf
            1.898596  #x-coord   noprior:  step:0.01
            3.007541  #y-coord   noprior:  step:0.01
           19.745753  #amp       noprior:  min:0  step:10
        psf
            4.972697  #x-coord   noprior:  step:0.01
            4.495651  #y-coord   noprior:  step:0.01
