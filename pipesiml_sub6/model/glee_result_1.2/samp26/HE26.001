    esource_sub_agn_psf_factor     1
    esource_sub_esr_psf  psf-s-26.fits
    esource_sub_esr_psf_factor     1
    esource_regtype      curv
    esource_regopt       SpecRegPrecSigFigOnce
    esource_reglam       1171.88
    esource_reglamlo     1e-12
    esource_reglamhi     100000
    esource_reglampre    1
    esource_reglamnup    1000
    esource_light  3
        sersic
            3.928669  #x-coord   link:lv1p1  a:0,1,1
            3.913648  #y-coord   link:lv1p2  a:0,1,1
            0.917992  #q         flat:0.3,1  step:0.03
            2.052457  #PA        noprior:  step:0.1
