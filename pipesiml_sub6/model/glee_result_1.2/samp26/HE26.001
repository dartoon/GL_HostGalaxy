            1.994447  #r_eff     flat:1.8,2.5  step:0.05
            4.361136  #n_sersic  flat:4,6  step:0.1
        psf
            1.904367  #x-coord   noprior:  step:0.01
            3.007691  #y-coord   noprior:  step:0.01
          804.268019  #amp       noprior:  min:0  step:10
        psf
            4.960120  #x-coord   noprior:  step:0.01
            4.479780  #y-coord   noprior:  step:0.01
         2004.105279  #amp       noprior:  min:0  step:20
