    esource_sub_esr_psf  psf-s-100.fits
    esource_sub_esr_psf_factor     1
    esource_regtype      curv
    esource_regopt       SpecRegPrecSigFigOnce
    esource_reglam       878.906
    esource_reglamlo     1e-12
    esource_reglamhi     100000
    esource_reglampre    1
    esource_reglamnup    1000
    esource_light  3
        sersic
            3.960705  #x-coord   link:lv1p1  a:0,1,1
            3.924521  #y-coord   link:lv1p2  a:0,1,1
            0.935007  #q         flat:0.3,1  step:0.03
            2.298890  #PA        noprior:  step:0.1
