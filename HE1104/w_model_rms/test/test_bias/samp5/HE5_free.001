# glee -M -i HE5_free

chi2type           16
minimiser       siman
seed                2

siman_iter          1
siman_nT         1000
siman_dS            5
siman_Sf          1.1
siman_k             2
siman_Ti           10
siman_Tf          1.2
siman_Tmin          1

mcmc_n          15000
mcmc_dS        0.55
mcmc_dSini          0
mcmc_k              2

sampling_f     gaussian
sampling_cov   HE5_free.cov

lenses_vary 2

    spemd
    3.922593  #x-coord   gaussian:3.94,0.08  label:lv1p1     step:0.08
    3.905262  #y-coord   gaussian:3.94,0.08  label:lv1p2     step:0.08
    0.655384  #b/a       flat:0.55,0.75  label:lv1p3     step:0.03
   -1.199362  #theta     noprior:  label:lv1p4     step:0.1
    1.736607  #theta_e   flat:1.5,2  scale:  step:0.1
  1.0000e-04  #r_core    exact:
    0.594067  #gam       flat:0.5,0.7  step:0.01

    shear
  1.0000e-06  #magnitude  exact:
    0.000000  #theta     exact:

esources 1

    Dds/Ds    1.000000  exact:
    esource_ngy          30
    esource_ngx          98
    esource_dx           0.080000
    esource_data         HE-5.fits
    esource_err          rms-5.fits
    esource_arcmask      mask-5.fits
    esource_lensmask     lensmask.fits
    esource_psf          psf-s-5.fits
    esource_sub_agn_psf  psf-s-5.fits
    esource_sub_agn_psf_factor     1
    esource_sub_esr_psf  psf-s-5.fits
    esource_sub_esr_psf_factor     1
    esource_regtype      curv
    esource_regopt       SpecRegPrecSigFigOnce
    esource_reglam       292.969
    esource_reglamlo     1e-12
    esource_reglamhi     100000
    esource_reglampre    1
    esource_reglamnup    1000
    esource_light  3
        sersic
            3.922593  #x-coord   link:lv1p1  a:0,1,1
            3.905262  #y-coord   link:lv1p2  a:0,1,1
            0.935283  #q         flat:0.3,1  step:0.03
            2.048607  #PA        noprior:  step:0.1
            0.134400  #amp       noprior:  min:0.001  step:0.1
            2.109994  #r_eff     flat:1.8,3  step:0.05
            4.637013  #n_sersic  flat:2,6  step:0.1
        psf
            1.901424  #x-coord   noprior:  step:0.01
            3.010555  #y-coord   noprior:  step:0.01
           10.902737  #amp       noprior:  min:0  step:10
        psf
            4.960808  #x-coord   noprior:  step:0.01
            4.483075  #y-coord   noprior:  step:0.01
           25.635200  #amp       noprior:  min:0  step:20
    esource_end
