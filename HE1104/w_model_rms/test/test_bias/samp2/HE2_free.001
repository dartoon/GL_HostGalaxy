# glee -M -i HE2_free

chi2type           16
minimiser       siman
seed                2

siman_iter          1
siman_nT         1000
siman_dS            5
siman_Sf          1.1
siman_k             2
siman_Ti           10
siman_Tf          1.2
siman_Tmin          1

mcmc_n          15000
mcmc_dS        0.10
mcmc_dSini          0
mcmc_k              2

sampling_f     gaussian
sampling_cov   HE2_free.cov

lenses_vary 2

    spemd
    3.930218  #x-coord   gaussian:3.94,0.08  label:lv1p1     step:0.08
    3.913929  #y-coord   gaussian:3.94,0.08  label:lv1p2     step:0.08
    0.642152  #b/a       flat:0.55,0.75  label:lv1p3     step:0.03
   -1.177333  #theta     noprior:  label:lv1p4     step:0.1
    1.739251  #theta_e   flat:1.5,2  scale:  step:0.1
  1.0000e-04  #r_core    exact:
    0.609014  #gam       flat:0.5,0.7  step:0.01

    shear
  1.0000e-06  #magnitude  exact:
    0.000000  #theta     exact:

esources 1

    Dds/Ds    1.000000  exact:
    esource_ngy          30
    esource_ngx          98
    esource_dx           0.080000
    esource_data         HE-2.fits
    esource_err          rms-2.fits
    esource_arcmask      mask-2.fits
    esource_lensmask     lensmask.fits
    esource_psf          psf-s-2.fits
    esource_sub_agn_psf  psf-s-2.fits
    esource_sub_agn_psf_factor     1
    esource_sub_esr_psf  psf-s-2.fits
    esource_sub_esr_psf_factor     1
    esource_regtype      curv
    esource_regopt       SpecRegPrecSigFigOnce
    esource_reglam       10.6812
    esource_reglamlo     1e-12
    esource_reglamhi     100000
    esource_reglampre    1
    esource_reglamnup    1000
    esource_light  3
        sersic
            3.930218  #x-coord   link:lv1p1  a:0,1,1
            3.913929  #y-coord   link:lv1p2  a:0,1,1
            0.994956  #q         flat:0.3,1  step:0.03
            2.103466  #PA        noprior:  step:0.1
            0.139364  #amp       noprior:  min:0.001  step:0.1
            2.275369  #r_eff     flat:1.8,3  step:0.05
            4.173779  #n_sersic  flat:2,6  step:0.1
        psf
            1.904848  #x-coord   noprior:  step:0.01
            3.017605  #y-coord   noprior:  step:0.01
           14.751985  #amp       noprior:  min:0  step:10
        psf
            4.962347  #x-coord   noprior:  step:0.01
            4.484160  #y-coord   noprior:  step:0.01
           25.552108  #amp       noprior:  min:0  step:20
    esource_end
