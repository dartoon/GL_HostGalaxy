# glee -M -i HE2.001

chi2type           16
minimiser       siman
seed                2

siman_iter          1
siman_nT         1000
siman_dS            5
siman_Sf          1.1
siman_k             2
siman_Ti           10
siman_Tf          1.2
siman_Tmin          1

mcmc_n          15000
mcmc_dS          0.15
mcmc_dSini          0
mcmc_k              2

sampling_f     gaussian
sampling_cov   HE2.001.cov

lenses_vary 2

    spemd
    3.924075  #x-coord   gaussian:3.94,0.08  label:lv1p1     step:0.08
    3.915011  #y-coord   gaussian:3.94,0.08  label:lv1p2     step:0.08
    0.713077  #b/a       flat:0.55,0.75  label:lv1p3     step:0.03
   -1.171834  #theta     noprior:  label:lv1p4     step:0.1
    1.698542  #theta_e   flat:1.5,1.7  scale:  step:0.1
  1.0000e-04  #r_core    exact:
    0.565022  #gam       flat:0.5,0.7  step:0.01

    shear
  1.0000e-06  #magnitude  exact:
    0.000000  #theta     exact:

esources 1

    Dds/Ds    1.000000  exact:
    esource_ngy          30
    esource_ngx          98
    esource_dx           0.080000
    esource_data         HE-2.fits
    esource_err          rms-2.fits
    esource_arcmask      mask-2.fits
    esource_lensmask     lensmask.fits
    esource_psf          psf-s-2.fits
    esource_sub_agn_psf  psf-s-2.fits
    esource_sub_agn_psf_factor     1
    esource_sub_esr_psf  psf-s-2.fits
    esource_sub_esr_psf_factor     1
    esource_regtype      curv
    esource_regopt       SpecRegPrecSigFigOnce
    esource_reglam       10.6812
    esource_reglamlo     1e-12
    esource_reglamhi     100000
    esource_reglampre    1
    esource_reglamnup    1000
    esource_light  3
        sersic
            3.924075  #x-coord   link:lv1p1  a:0,1,1
            3.915011  #y-coord   link:lv1p2  a:0,1,1
            0.981060  #q         flat:0.3,1  step:0.03
            2.179088  #PA        noprior:  step:0.1
            0.129085  #amp       noprior:  min:0.001  step:0.1
            2.490870  #r_eff     flat:1.8,2.5  step:0.05
            4.004375  #n_sersic  flat:4,6  step:0.1
        psf
            1.887345  #x-coord   noprior:  step:0.01
            3.004875  #y-coord   noprior:  step:0.01
           17.519344  #amp       noprior:  min:0  step:10
        psf
            4.958915  #x-coord   noprior:  step:0.01
            4.462462  #y-coord   noprior:  step:0.01
           27.433371  #amp       noprior:  min:0  step:20
    esource_end
