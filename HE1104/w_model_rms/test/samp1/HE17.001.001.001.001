    esource_sub_agn_psf_factor     1
    esource_sub_esr_psf  psf-s-17.fits
    esource_sub_esr_psf_factor     1
    esource_regtype      curv
    esource_regopt       SpecRegPrecSigFigOnce
    esource_reglam       3.8147
    esource_reglamlo     1e-12
    esource_reglamhi     100000
    esource_reglampre    1
    esource_reglamnup    1000
    esource_light  3
        sersic
            3.930646  #x-coord   link:lv1p1  a:0,1,1
            3.919965  #y-coord   link:lv1p2  a:0,1,1
            0.999652  #q         flat:0.3,1  step:0.03
            1.921150  #PA        noprior:  step:0.1
            0.138769  #amp       noprior:  min:0.001  step:0.1
            2.499942  #r_eff     flat:1.8,2.5  step:0.05
            4.002960  #n_sersic  flat:4,6  step:0.1
        psf
            1.906417  #x-coord   noprior:  step:0.01
            3.014153  #y-coord   noprior:  step:0.01
           18.208881  #amp       noprior:  min:0  step:10
        psf
            4.958922  #x-coord   noprior:  step:0.01
            4.493177  #y-coord   noprior:  step:0.01
           30.034938  #amp       noprior:  min:0  step:20
