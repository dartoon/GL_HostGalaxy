    esource_reglam       878.906
    esource_reglamlo     1e-12
    esource_reglamhi     100000
    esource_reglampre    1
    esource_reglamnup    1000
    esource_light  3
        sersic
            3.958535  #x-coord   link:lv1p1  a:0,1,1
            3.922162  #y-coord   link:lv1p2  a:0,1,1
            0.912478  #q         flat:0.3,1  step:0.03
