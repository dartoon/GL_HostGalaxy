    esource_sub_agn_psf  psf-s-86.fits
    esource_sub_agn_psf_factor     1
    esource_sub_esr_psf  psf-s-86.fits
    esource_sub_esr_psf_factor     1
    esource_regtype      curv
    esource_regopt       SpecRegPrecSigFigOnce
    esource_reglam       488.281
    esource_reglamlo     1e-12
    esource_reglamhi     100000
    esource_reglampre    1
    esource_reglamnup    1000
    esource_light  3
        sersic
            3.957730  #x-coord   link:lv1p1  a:0,1,1
            3.922867  #y-coord   link:lv1p2  a:0,1,1
            0.927184  #q         flat:0.3,1  step:0.03
            2.041758  #PA        noprior:  step:0.1
            0.155375  #amp       noprior:  min:0.001  step:0.1
            1.945898  #r_eff     flat:1.8,2.5  step:0.05
            4.721715  #n_sersic  flat:4,6  step:0.1
        psf
            1.635869  #x-coord   noprior:  step:0.01
            2.883042  #y-coord   noprior:  step:0.01
          810.326067  #amp       noprior:  min:0  step:10
        psf
            4.799898  #x-coord   noprior:  step:0.01
            4.399686  #y-coord   noprior:  step:0.01
