            4.309760  #x-coord   noprior:  step:0.01
            5.128781  #y-coord   noprior:  step:0.01
          296.062171  #amp       noprior:  min:0  step:10
        psf
            5.253686  #x-coord   noprior:  step:0.01
            3.796017  #y-coord   noprior:  step:0.01
          336.533396  #amp       noprior:  min:0  step:10
        psf
            2.880154  #x-coord   noprior:  step:0.01
            4.478286  #y-coord   noprior:  step:0.01
