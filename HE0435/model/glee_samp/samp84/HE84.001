    esource_sub_esr_psf  psf-s-84.fits
    esource_sub_esr_psf_factor     1
    esource_regtype      curv
    esource_regopt       SpecRegPrecSigFigOnce
    esource_reglam       683.594
    esource_reglamlo     1e-12
    esource_reglamhi     100000
    esource_reglampre    1
    esource_reglamnup    1000
    esource_light  5
        sersic
            3.980909  #x-coord   link:lv1p1  a:0,1,1
            4.012088  #y-coord   link:lv1p2  a:0,1,1
            0.804648  #q         flat:0.5,1  step:0.03
            1.382297  #PA        noprior:  step:0.1
