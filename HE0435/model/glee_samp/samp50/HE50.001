    esource_sub_esr_psf  psf-s-50.fits
    esource_sub_esr_psf_factor     1
    esource_regtype      curv
    esource_regopt       SpecRegPrecSigFigOnce
    esource_reglam       91.5527
    esource_reglamlo     1e-12
    esource_reglamhi     100000
    esource_reglampre    1
    esource_reglamnup    1000
    esource_light  5
        sersic
            4.016249  #x-coord   link:lv1p1  a:0,1,1
            3.960208  #y-coord   link:lv1p2  a:0,1,1
            0.806643  #q         flat:0.5,1  step:0.03
