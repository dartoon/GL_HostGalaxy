            5.127797  #y-coord   noprior:  step:0.01
          305.855545  #amp       noprior:  min:0  step:10
        psf
            5.252122  #x-coord   noprior:  step:0.01
            3.796900  #y-coord   noprior:  step:0.01
          341.725396  #amp       noprior:  min:0  step:10
        psf
            2.881553  #x-coord   noprior:  step:0.01
