# glee -M -i HE3

chi2type           16
minimiser       siman
seed                1

siman_iter          1
siman_nT         1000
siman_dS            5
siman_Sf          1.1
siman_k             2
siman_Ti           10
siman_Tf          1.2
siman_Tmin          1

mcmc_n           15000
mcmc_dS          0.02
mcmc_dSini          0
mcmc_k              2

lenses_vary 2

    spemd
    3.982073  #x-coord   gaussian:3.9,0.08  label:lv1p1     step:0.08
    3.950479  #y-coord   gaussian:3.9,0.08  label:lv1p2     step:0.08
    0.828255  #b/a       flat:0.65,0.95  label:lv1p3     step:0.03
    1.290132  #theta     noprior:  label:lv1p4     step:0.1
    1.401265  #theta_e   flat:1.1,1.7  scale:  step:0.1
  1.0000e-04  #r_core    exact:
    0.422818  #gam       flat:0.2,0.6  step:0.01

    shear
  1.0000e-06  #magnitude  exact:
    0.000000  #theta     exact:

esources 1

    Dds/Ds    1.000000  exact:
    esource_ngy          30
    esource_dx           0.080000
    esource_data         HE-3.fits
    esource_err          rms-3.fits
    esource_arcmask      mask-3.fits
    esource_psf          psf-s-3.fits
    esource_regtype      curv
    esource_regopt       SpecRegPrecSigFigOnce
    esource_reglam       0.762939
    esource_reglamlo     1e-12
    esource_reglamhi     100000
    esource_reglampre    1
    esource_reglamnup    1000
    esource_end
