# glee -M -i HE4

chi2type           16
minimiser       siman
seed                1

siman_iter          1
siman_nT         1000
siman_dS            5
siman_Sf          1.1
siman_k             2
siman_Ti           10
siman_Tf          1.2
siman_Tmin          1

mcmc_n           15000
mcmc_dS          0.05
mcmc_dSini          0
mcmc_k              2

lenses_vary 2

    spemd
    3.981295  #x-coord   gaussian:3.9,0.08  label:lv1p1     step:0.08
    3.953999  #y-coord   gaussian:3.9,0.08  label:lv1p2     step:0.08
    0.823516  #b/a       flat:0.65,0.95  label:lv1p3     step:0.03
    1.282615  #theta     noprior:  label:lv1p4     step:0.1
    1.400067  #theta_e   flat:1.1,1.7  scale:  step:0.1
  1.0000e-04  #r_core    exact:
    0.426073  #gam       flat:0.2,0.6  step:0.01

    shear
  1.0000e-06  #magnitude  exact:
    0.000000  #theta     exact:

esources 1

    Dds/Ds    1.000000  exact:
    esource_ngy          30
    esource_dx           0.080000
    esource_data         HE-4.fits
    esource_err          rms-4.fits
    esource_arcmask      mask-4.fits
    esource_psf          psf-s-4.fits
    esource_regtype      curv
    esource_regopt       SpecRegPrecSigFigOnce
    esource_reglam       3.8147
    esource_reglamlo     1e-12
    esource_reglamhi     100000
    esource_reglampre    1
    esource_reglamnup    1000
    esource_end
