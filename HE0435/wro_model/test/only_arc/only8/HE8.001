# glee -M -i HE8

chi2type           16
minimiser       siman
seed                1

siman_iter          1
siman_nT         1000
siman_dS            5
siman_Sf          1.1
siman_k             2
siman_Ti           10
siman_Tf          1.2
siman_Tmin          1

mcmc_n           15000
mcmc_dS           0.2
mcmc_dSini          0
mcmc_k              2

lenses_vary 2

    spemd
    3.960531  #x-coord   gaussian:3.9,0.08  label:lv1p1     step:0.08
    3.946978  #y-coord   gaussian:3.9,0.08  label:lv1p2     step:0.08
    0.737280  #b/a       flat:0.65,0.95  label:lv1p3     step:0.03
    1.288655  #theta     noprior:  label:lv1p4     step:0.1
    1.456088  #theta_e   flat:1.1,1.7  scale:  step:0.1
